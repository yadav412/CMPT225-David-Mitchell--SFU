#include <iostream>
#include <stack>
#include <cstring>
#include <queue> 


using namespace std;


int main()
{
	stack<int> s;
	s.push(6);//6
	s.pop();//
	s.push(9);//9
	s.push(1);// 1,9

	cout<<s.top()<<endl;// returns 1

	stack<char> d;
	d.push('i');//'i'
	cout<<d.empty()<<endl; //returns a boolean value - 0 returned 
	cout<<d.top()<<endl ; //'i' value returned

	queue<int> f; 

	f.push(5);
	f.push(9);
	f.push(8);
	cout <<f.front()<<endl;// produces 5 
	cout <<f.back()<<endl;//produces 8

	queue<char> g;

	g.push('g');

	cout<<g.back()<<endl;
	cout<<g.size();

	system("pause");
	return 0;
}
